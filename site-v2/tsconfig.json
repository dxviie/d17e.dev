{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "strictNullChecks": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "paths": {
      "astro/loaders": [
        "./node_modules/astro/dist/content/loaders"
      ],
      "astro/config": [
        "./node_modules/astro/dist/config"
      ]
    }
  }
}
