---
import {SITE_TITLE, SITE_DESCRIPTION} from '$root/src/consts';
import {getCollection} from 'astro:content';

const articles = (await getCollection('articles'));
---

<!doctype html>
<html lang="en">
<body>
<main>
  <section>
    <ul>
      {
        articles.map((article) => (
                <li>{article.id}
                  {article.data.cover.title}
                </li>
        ))
      }
    </ul>
  </section>
</main>
</body>
</html>
